{
  "name": "@svrooij/sonos-docs",
  "description": "CLI tool to generate documentation for the undocumented sonos api",
  "version": "0.0.0",
  "author": "Stephan van Rooij @svrooij",
  "bin": {
    "sonos-docs": "./bin/run"
  },
  "bugs": "https://github.com/svrooij/sonos-api-docs/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "cli-ux": "^5.5.0",
    "fast-xml-parser": "^3.17.4",
    "handlebars": "^4.7.6",
    "node-fetch": "^2.6.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@svrooij/sonos": "^2.1.1",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.44",
    "@types/node-fetch": "^2.5.7",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/templates",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/svrooij/sonos-api-docs",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sonos-docs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/svrooij/sonos-api-docs",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "intermediate": "./bin/run combine --docsFile=../../docs/documentation.json --folder=data",
    "intermediate-win": ".\\bin\\run combine --docsFile=../../docs/documentation.json --folder=data",
    "combine-win": ".\\bin\\run combine --docsFile=../../docs/documentation.json --folder=data",
    "docs": "./bin/run generate docs ../../docs",
    "docs-win": ".\\bin\\run generate docs ../../docs"
  },
  "types": "lib/index.d.ts"
}
